        <attr name="pinPrimaryColor" format="color" />
        <attr name="pinSecondaryColor" format="color" />
        <attr name="android:radius" format="dimension" />
        <attr name="android:textSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="PinCode">
        <attr name="pinInputsTextColor" format="color" />
        <attr name="isPinHidden" format="boolean" />
