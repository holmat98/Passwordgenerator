<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PinEditText">
        <attr name="pinBackgroundColor" format="color" />
        <attr name="pinTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="PinCode">
        <attr name="areInputsFocusable" format="boolean" />
        <attr name="pinInputsBackgroundColor" format="color" />
        <attr name="pinInputsTextColor" format="color" />
        <attr name="isPinHidden" format="boolean" />
    </declare-styleable>
    <declare-styleable name="DoubleButton">
        <attr name="firstText" format="string" />
        <attr name="secondText" format="string" />
        <attr name="firstBackground" format="color" />
        <attr name="secondBackground" format="color" />
        <attr name="firstTextColor" format="color" />
        <attr name="secondTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="AnimatedCheckbox">
        <attr name="text" format="string" />
    </declare-styleable>
</resources>